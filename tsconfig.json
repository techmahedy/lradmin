{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],                               
    "esModuleInterop": true,    
    "module": "esnext",                     
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": false,
    "noImplicitAny": false,
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],                        
  },
  "include": [
    "resources/js",
    "table.md", 
    "table.md"  
  ],
  "exclude": [
    "node_modues"
  ]   
}